set number
set noswapfile
set tabstop=4
set softtabstop=4
set shiftwidth=4
set autoindent
set wildmode=longest,list
set mouse=a
set cursorline
set showmatch
let python_highlight_all=1
let mapleader="\<space>"
set timeout timeoutlen=1000
filetype plugin indent on
syntax on
nnoremap <C-Up> 5k
nnoremap <C-down> 5j
set clipboard=unnamedplus
set encoding=UTF-8
